{
  "name": "shopify-liquid-theme",
  "version": "0.0.0",
  "description": "Bedrock on Liquid Shopify Theme",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/bvaccel-delivery/shopify-liquid-mono.git"
  },
  "license": "UNLICENSED",
  "author": "Accenture",
  "scripts": {
    "dev": "run-p -s watch-dev __theme-dev-delayed",
    "dev-prod": "run-p -s watch-prod __theme-dev-delayed",
    "__stub-dist": "mkdir -p dist/{assets,config,layout,locales,sections,snippets,templates}",
    "__theme-dev-delayed": "delay 10 && npm run theme-dev",
    "theme-dev": "dotenv -- bash -c 'shopify theme dev -e $SHOPIFY_CLI_ENV'",
    "push-theme": "dotenv -- bash -c 'run-s build-prod && shopify theme push -e $SHOPIFY_CLI_ENV --nodelete'",
    "push-theme-unpublished": "dotenv -- bash -c 'run-s build-prod && shopify theme push -e $SHOPIFY_CLI_ENV --nodelete --unpublished'",
    "pull-merch": "dotenv -- bash -c 'shopify theme pull -e $SHOPIFY_CLI_ENV --path ./src/$THEME_OVERRIDE_DIR/shopify --nodelete -o templates/*.json config/settings_data.json locales/*.json'",
    "pull-merch:config": "dotenv -- bash -c 'shopify theme pull -e $SHOPIFY_CLI_ENV --path ./src/$THEME_OVERRIDE_DIR/shopify --nodelete -o config/settings_data.json'",
    "pull-merch:templates": "dotenv -- bash -c 'shopify theme pull -e $SHOPIFY_CLI_ENV --path ./src/$THEME_OVERRIDE_DIR/shopify --nodelete -o templates/*.json'",
    "pull-merch:locales": "dotenv -- bash -c 'shopify theme pull -e $SHOPIFY_CLI_ENV --path ./src/$THEME_OVERRIDE_DIR/shopify --nodelete -o locales/*.json'",
    "clean-merch": "run-s -c clean-merch:*",
    "clean-merch:config": "dotenv -- bash -c 'rm -f ./src/$THEME_OVERRIDE_DIR/shopify/config/settings_data.json'",
    "clean-merch:templates": "dotenv -- bash -c 'rm -f ./src/$THEME_OVERRIDE_DIR/shopify/templates/*.json && rm -rf ./src/$THEME_OVERRIDE_DIR/shopify/templates/**/*.json'",
    "clean-merch:locales": "dotenv -- bash -c 'rm -f ./src/$THEME_OVERRIDE_DIR/shopify/locales/*.json'",
    "lint-js": "eslint \"src/**/*.{js,vue}\"",
    "lint-css": "stylelint \"src/**/*.{vue,css,sass,scss}\"",
    "lint-theme": "dotenv -- bash -c 'shopify theme check -e $SHOPIFY_CLI_ENV'",
    "fix-js": "eslint \"src/**/*.{js,vue}\" --fix",
    "fix-css": "stylelint \"src/**/*.{vue,css,sass,scss}\" --fix",
    "fix-theme": "dotenv -- bash -c 'shopify theme check -e $SHOPIFY_CLI_ENV -a'",
    "watch-dev": "cross-env NODE_ENV=development webpack --config ./webpack.dev.js --watch --progress",
    "watch-prod": "cross-env NODE_ENV=development webpack --config ./webpack.prod.js --watch --progress",
    "build-dev": "cross-env NODE_ENV=development webpack --config ./webpack.dev.js --progress",
    "build-prod": "cross-env NODE_ENV=production webpack --config ./webpack.prod.js --progress",
    "build-icons": "gulp icons:build",
    "help": "cat package-scripts-help.txt"
  },
  "config": {
    "prod-store-url": "fab-development.myshopify.com"
  },
  "dependencies": {
    "@bva/ui-shared": "^1.4.22",
    "@bva/ui-vue": "^1.4.14",
    "aos": "^2.3.4",
    "axios": "^1.3.2",
    "dialog-polyfill": "^0.5.6",
    "install": "^0.13.0",
    "nouislider": "^15.7.0",
    "pinia": "^2.0.23",
    "swiper": "^9.1.0",
    "tailwindcss": "^3.0.23",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@shopify/prettier-plugin-liquid": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "delay-cli": "^2.0.0",
    "dotenv": "^16.1.4",
    "dotenv-cli": "^7.2.1",
    "ejs": "^3.1.8",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-inject": "^5.0.5",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.0",
    "revolver-webpack-plugin": "^1.0.4",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-webpack-plugin": "^3.2.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-shell-plugin-next": "^1.2.0"
  }
}
