{
  "name": "shopify-liquid-theme",
  "version": "0.0.0",
  "description": "The Stable Shopify Theme",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/bvaccel-delivery/shopify-liquid-mono.git"
  },
  "license": "UNLICENSED",
  "author": "The Stable",
  "config": {
    "prod-store-url": "fab-development.myshopify.com"
  },
  "scripts": {
    "test": "echo $npm_package_config_prod_store_url",
    "start": "run-p -s webpack:watch serve",
    "serve": "shopify3 theme dev --store $npm_package_config_store_url --path ./dist",
    "deploy": "run-s webpack:build && shopify3 theme push --store $npm_package_config_prod_store_url --path ./dist --nodelete",
    "deploy:new": "run-s webpack:build && shopify3 theme push --store $npm_package_config_prod_store_url --path ./dist --nodelete --unpublished",
    "pull-theme": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify",
    "pull-merch": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify --nodelete -o templates/*.json config/settings_data.json locales/*.json",
    "pull:templates": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify --nodelete -o templates/*.json",
    "pull:config": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify --nodelete -o config/settings_data.json",
    "pull:locales": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify --nodelete -o locales/*.json",
    "pull-dev-merch": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify --nodelete --development -o templates/*.json config/settings_data.json locales/*.json",
    "pull-dev:templates": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify --development --nodelete -o templates/*.json",
    "pull-dev:config": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify --development --nodelete -o config/settings_data.json",
    "pull-dev:locales": "shopify3 theme pull --store $npm_package_config_prod_store_url --path ./src/theme-override/shopify --development --nodelete -o locales/*.json",
    "clean-merch": "run-s -c clean:*",
    "clean:templates": "rm -f ./src/theme-override/shopify/templates/**/*.json",
    "clean:config": "rm -f ./src/theme-override/shopify/config/settings_data.json",
    "clean:locales": "rm -f ./src/theme-override/shopify/locales/*.json",
    "lint": "run-s -c lint:*",
    "lint:js": "eslint \"src/**/*.{js,vue}\"",
    "lint:css": "stylelint \"src/**/*.{vue,css,sass,scss}\"",
    "lint:shopify": "shopify3 theme check --path ./dist",
    "fix": "run-s -c fix:*",
    "fix:js": "eslint \"src/**/*.{js,vue}\" --fix",
    "fix:css": "stylelint \"src/**/*.{vue,css,sass,scss}\" --fix",
    "fix:shopify": "shopify3 theme check --path ./dist -a",
    "webpack:watch": "cross-env NODE_ENV=development BROWSERSLIST_ENV=development BROWSERSLIST_CONFIG=./.browserslistrc webpack --config ./webpack.dev.js --watch --progress",
    "webpack:build": "cross-env NODE_ENV=production BROWSERSLIST_ENV=production BROWSERSLIST_CONFIG=./.browserslistrc webpack --config ./webpack.prod.js --progress",
    "gulp:icons": "gulp icons:build",
    "gulp:icons-watch": "gulp icons:watch",
    "help": "if ! command -v jq &> /dev/null; then cat ./package.json; exit 0; fi && cat ./package.json | jq .scripts"
  },
  "dependencies": {
    "@bva/ui-shared": "^1.4.6",
    "@bva/ui-vue": "^1.4.6",
    "axios": "^1.3.2",
    "install": "^0.13.0",
    "nouislider": "^15.7.0",
    "pinia": "^2.0.23",
    "swiper": "^9.1.0",
    "tailwindcss": "^3.0.23",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@shopify/prettier-plugin-liquid": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "ejs": "^3.1.8",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-inject": "^5.0.5",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.0",
    "revolver-webpack-plugin": "^1.0.4",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-webpack-plugin": "^3.2.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-shell-plugin-next": "^1.2.0"
  }
}
