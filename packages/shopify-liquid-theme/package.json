{
  "name": "nanit-shopify-liquid",
  "description": "Shopify theme for Nanit",
  "author": "The Stable",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "repository": "https://bitbucket.org/bvaccel-delivery/nanit-shopify-liquid/src/develop",
  "scripts": {
    "login:dev": "shopify login --store aislings-sweets-treats.myshopify.com",
    "login:prod": "shopify login --store aislings-sweets-treats.myshopify.com",
    "start": "run-p -sr webpack:watch serve",
    "serve": "cd ./dist && shopify theme serve",
    "deploy": "run-s webpack:build && cd ./dist && shopify theme push --nodelete",
    "deploy:new": "run-s webpack:build && cd ./dist && shopify theme push --nodelete --unpublished",
    "pull-theme": "cd ./src/shopify && shopify theme pull",
    "pull-merch": "cd ./src/shopify && shopify theme pull --nodelete -o templates/*.json config/settings_data.json locales/*.json",
    "pull:templates": "cd ./src/shopify && shopify theme pull --nodelete -o templates/*.json",
    "pull:config": "cd ./src/shopify && shopify theme pull --nodelete -o config/settings_data.json",
    "pull:locales": "cd ./src/shopify && shopify theme pull --nodelete -o locales/*.json",
    "pull-dev-merch": "cd ./src/shopify && shopify theme pull --nodelete --development -o templates/*.json config/settings_data.json locales/*.json",
    "pull-dev:templates": "cd ./src/shopify && shopify theme pull --development --nodelete -o templates/*.json",
    "pull-dev:config": "cd ./src/shopify && shopify theme pull --development --nodelete -o config/settings_data.json",
    "pull-dev:locales": "cd ./src/shopify && shopify theme pull --development --nodelete -o locales/*.json",
    "clean-merch": "run-s -c clean:*",
    "clean:templates": "cd ./src/shopify && rm -f ./templates/*.json",
    "clean:config": "cd ./src/shopify && rm -f ./config/settings_data.json",
    "clean:locales": "cd ./src/shopify && rm -f ./locales/*.json",
    "lint": "run-s -c lint:*",
    "lint:js": "eslint \"{src/**/*}.{js,vue}\"",
    "lint:css": "stylelint \"{src/**/*}.{vue,css,sass,scss}\"",
    "lint:shopify": "cd ./dist && shopify theme check",
    "fix": "run-s -c fix:*",
    "fix:js": "eslint \"{src/**/*}.{js,vue}\" --fix",
    "fix:css": "stylelint \"{src/**/*}.{vue,css,sass,scss}\" --fix",
    "fix:shopify": "cd ./dist && shopify theme check -a",
    "webpack:watch": "cross-env NODE_ENV=development BROWSERSLIST_ENV=development BROWSERSLIST_CONFIG=./.browserslistrc webpack --config ./webpack.dev.js --watch --progress",
    "webpack:build": "cross-env NODE_ENV=production BROWSERSLIST_ENV=production BROWSERSLIST_CONFIG=./.browserslistrc webpack --config ./webpack.prod.js --progress",
    "gulp:icons": "gulp icons:build",
    "gulp:icons-watch": "gulp icons:watch",
    "help": "if ! command -v jq &> /dev/null; then cat ./package.json; exit 0; fi && cat ./package.json | jq .scripts"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "ejs": "^3.1.8",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-inject": "^5.0.5",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-webpack-plugin": "^3.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "install": "^0.13.0",
    "tailwindcss": "^3.0.23",
    "vue": "^3.2.31"
  }
}
